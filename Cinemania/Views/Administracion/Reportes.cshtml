
@{
    Layout = "/Views/Shared/_LayoutAdministracion.cshtml";
}

<div class="container">
    <form action="./Administracion/ListaReportes" method="post">
        <div id="loginbox" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
            <div class="form-group">
                <label for="Desde">Desde: </label>
                <input type="date" id="FechaDesde" name="FechaDesde" />
            </div>

            <div class="form-group">
                <label for="Hasta">Hasta: </label>
                <input type="date" id="FechaHasta" name="FechaHasta" />
            </div>

            <div class="text-center">
                <input type="submit" class="btn btn-success" value="Buscar" />

                <!-- LINK SALIR -->
                <a href="./Administracion/Inicio" class="btn btn-default">Cancelar</a>
            </div>
        </div>
    </form>

    <span class="text-center">
        @TempData["Error"]
    </span>
</div>